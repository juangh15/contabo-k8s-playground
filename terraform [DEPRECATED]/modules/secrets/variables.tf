variable "contabo_main_ssh_key_name" {
  description = "Name of the main_ssh_key to display on Contabo secrets console"
  type        = string
  default     = "main_ssh_key"
}

variable "contabo_main_root_password_name" {
  description = "Name of the main_root_password to display on Contabo secrets console"
  type        = string
  default     = "main_root_password"
}